<!DOCTYPE html>
<html>
<head>
  <title>Hello</title>
  <%= stylesheet_link_tag  'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if logged_in? %>
  <%= link_to "Sign Out", session_url, method: "delete" %>
<% else %>
  <%= link_to "Sign Up", new_user_url %> or <%= link_to "Sign In", new_session_url %>
<% end %>
<br>

<%= render "shared/errors" %>

<%= yield %>

</body>
</html>
